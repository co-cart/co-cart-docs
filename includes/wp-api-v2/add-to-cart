<span class="k">print</span><span class="p">(</span><span class="n">wcapi</span><span class="o">.</span><span class="n">post</span><span class="p">(</span><span class="s">"cart/add"</span><span class="p">,</span> <span class="n">data</span><span class="p">)</span><span class="o">.</span><span class="n">json</span><span class="p">())</span>
</code></pre><pre class="highlight ruby tab-ruby"><code><span class="n">data</span> <span class="o">=</span> <span class="p">{</span>
